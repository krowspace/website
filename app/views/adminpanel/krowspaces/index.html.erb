<h1>Krowspaces</h1>

<table>
	<thead>
		<tr>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @krowspaces.each do |krowspace| %>
		<tr>
			<td><%= krowspace.name %></td>
			<td><%= krowspace.location %></td>
			<td><%= krowspace.seat_number %></td>
			<td><%= link_to 'Show', adminpanel_krowspace_path(krowspace) %></td>
			<td><%= link_to 'Edit', edit_adminpanel_krowspace_path(krowspace) %></td>
			<td><%= link_to 'Destroy', adminpanel_krowspace_path(krowspace), method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
		<% end %>
	</tbody>
</table>

<br>

<%= link_to 'New Krowspace', new_adminpanel_krowspace_path %>
